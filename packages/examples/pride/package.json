{
  "name": "traclabs-pride-xr",
  "version": "0.1.0",
  "browserslist": [
    "Edge 18"
  ],
  "dependencies": {
    "@ethereal/parcel-resolver-esm": "file:../../parcel-resolver-esm",
    "@vue/composition-api": "^1.0.0-beta.11",
    "cannon": "0.6.2",
    "ethereal": "1.1.2",
    "three": "0.113.2",
    "three-mesh-bvh": "0.1.3",
    "three-stl-loader": "1.0.6",
    "three-web-layer": "2.0.13",
    "vue": "3.0.0-rc.10",
    "yeux": "1.0.1"
  },
  "devDependencies": {
    "@types/cannon": "0.1.3",
    "@types/node": "13.7.1",
    "@vue/compiler-sfc": "3.0.0-rc.10",
    "autoprefixer": "9.7.1",
    "rimraf": "3.0.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "private": true,
  "scripts": {
    "build": "parcel build src/index.html --dist-dir dist --public-url /ethereal/pride-xr/",
    "prebuild": "rimraf dist && mkdir dist && ln -sf ../src/static ./dist/static",
    "start": "npm run prebuild && parcel src/index.html --dist-dir dist --host 0.0.0.0 --https"
  }
}
