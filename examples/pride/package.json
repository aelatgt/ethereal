{
  "name": "ethereal-example-traclabs-pride-xr",
  "version": "0.1.0",
  "babel": {
    "presets": [
      [
        "@babel/preset-env"
      ]
    ]
  },
  "browserslist": [
    "Edge 18"
  ],
  "dependencies": {
    "@types/cannon": "^0.1.3",
    "@vue/compiler-sfc": "^3.0.0-rc.10",
    "autoprefixer": "^9.7.1",
    "cannon": "^0.6.2",
    "ethereal": "^1.0.0",
    "postcss": "^8.1.7",
    "three-mesh-bvh": "^0.2.0",
    "three-stl-loader": "^1.0.6",
    "vue": "^3.0.0-rc.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "private": true,
  "scripts": {
    "prebundle": "rimraf dist && mkdir dist && ln -sf ../src/static ./dist/static",
    "bundle": "parcel build src/index.html --target app --dist-dir dist --public-url /ethereal/pride-xr/",
    "start": "npm run prebundle && parcel src/index.html --target app --no-hmr --dist-dir dist --https --host 0.0.0.0"
  },
  "targets": {
    "app": {}
  }
}
